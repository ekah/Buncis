<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <section name="NHibernate.Burrow" type="NHibernate.Burrow.Configuration.NHibernateBurrowCfgSection, NHibernate.Burrow" />
  </configSections>
  <NHibernate.Burrow customConfigurator="Buncis.Data.Common.NHibernateConfigurator, Buncis.Data.Common"></NHibernate.Burrow>
  <appSettings>
    <add key="PpnTax" value="10.00" />
    <add key="GlobalItemPerPage" value="10" />
    <add key="DeveloperEmail" value="email@test.com" />
  </appSettings>
  <connectionStrings>
    <add name="BuncisConnectionString" connectionString="Data Source=localhost;Initial Catalog=Buncis;User Id=sa;Password=root" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0"></compilation>
    <authentication mode="Forms">
      <forms loginUrl="/Buncis/Account/Login.aspx" timeout="2880" />
    </authentication>
    <membership defaultProvider="BuncisSqlMembershipProvider">
      <providers>
        <clear />
        <add name="BuncisSqlMembershipProvider" type="System.Web.Security.SqlMembershipProvider" connectionStringName="BuncisConnectionString" requiresUniqueEmail="true" minRequiredPasswordLength="6" minRequiredNonalphanumericCharacters="0" applicationName="/" />
      </providers>
    </membership>
    <profile defaultProvider="BuncisSqlProfileProvider">
      <providers>
        <clear />
        <add name="BuncisSqlProfileProvider" type="System.Web.Profile.SqlProfileProvider" connectionStringName="BuncisConnectionString" applicationName="/" />
      </providers>
    </profile>
    <roleManager enabled="true" defaultProvider="BuncisSqlRoleProvider">
      <providers>
        <clear />
        <add name="BuncisSqlRoleProvider" type="System.Web.Security.SqlRoleProvider" connectionStringName="BuncisConnectionString" applicationName="/" />
      </providers>
    </roleManager>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
      <controls>
        <add tagName="NewsList" tagPrefix="bun" src="~/UserControls/News/NewsList.ascx" />
      </controls>
    </pages>
  </system.web>
  <location allowOverride="true">
    <system.web>
      <authorization>
        <allow users="?" />
        <allow users="*" />
      </authorization>
    </system.web>
  </location>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"></modules>
    <handlers>
      <remove name="UrlRoutingHandler"></remove>
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.5.1.0" newVersion="2.5.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>